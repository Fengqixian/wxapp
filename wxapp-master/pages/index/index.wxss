

.searchbar-result{
    margin-top: 0;
    font-size: 14px;
}
.searchbar-result:before{
    display: none;
}
.weui-cell{
    padding: 12px 15px 12px 35px;
}

.SelectStationMap{
  width: 100%;
  height: 92vh;
  z-index: 0;
}
.fueltype_border{
  margin-top: 4vh;
  margin-left: 4vh;
   width: 7vh;
  height: 4vh;
  border:2px solid black;
}
.fueltype{
  display: block;
  margin-top: 0.5vh;
  margin-left: 0.5vh;
  font-family: '宋体';
  font-weight: bold;
  font-size: 18px;
  text-align: center;
  color: white;
  width: 6vh;
  height: 3vh;
  background-color: black;
}
.station_select{
  width: 55vh;

  margin-left:3vh;
  
  position: absolute;
  bottom: 0;
  z-index: 9999;
}
.station_info{
  display: blocak;
  width: 100vh;
  height: 10vh;
  border: 1px solid #CFCFCF;
 
  background-color: #F7F7F7;
 
}
.user_info{
  display: block;
   width: 9vh;
  height: 6vh;

  position: absolute;
  right: 0;
  margin-top:-6vh; 
  margin-right:4vh;
}
.cover_image{
  width: 5vh;
  height: 5vh;
  margin-left:2vh; 
  margin-top:1vh; 
}
.up_image{
  margin: 0 auto;
  width: 5vh;
  height: 3vh;
 
}
.i_address{
  margin-top: 4vh;
  margin-left: 3vh;
   width: 9vh;
  height: 6vh;
  
}
.click_button{
  
  width: 8vh;
  height: 2.3vh;
  border: 1px solid #A9A9A9;
  background-color:#D1D1D1; 
  margin: 0 auto;
}
.cview_title{
  
  font-family: "微软雅黑";
  font-weight: bold;
  margin-left: 10vh;
  margin-top: 1vh;
}
.cview_price{
  margin-top:2vh; 
  font-family: "微软雅黑";
  font-size: 12px;
  margin-left: 10vh;
  font-weight: blod;
}
.station_logo{
  position: absolute;
  display: block;
  width: 5vh;
  height: 5vh;
  margin-top:2vh;
  margin-left:2vh;
}
.number_1{
  position: absolute;
  width:20vh;
  height: 4vh;

  margin-left: 12vh;
  margin-top: -3vh;
  font-family: "微软雅黑";
  font-size: 20px;
  text-align: center;
  font-weight: bold;

  color: red;
}

.distance_log{
  position: absolute;
  width: 11vh;
  height: 3vh;

  right: 0;
  margin-top:-5vh;
}
.distance{
  position: absolute;
  width: 8vh;
  height: 3vh;
  font-family: '微软雅黑';
  right: 0;
  margin-top:-4vh;
  color: #9E9E9E;
  font-size: 14px;
  font-weight: bold;
  
}